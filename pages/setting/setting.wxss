/* pages/login.wxss */

page {
  width: 100%;
  height: 100%;

}


.section {
  min-width: 100%;
}

.section_height {
  min-width: 100%;
  height: 100%;
}

.setting{
  position: absolute;
  right: 0;
  margin-right: 5%;
  width: 8%;
  height: 4%;
}



.box{
  color: #6699ff;
  height: 50px;
  margin-left: 12%;
  margin-right: 12%;
  background-color: white;
}


.bc_white {
  background: white;
}

.bc_blue {
  background: #6699ff;
}


.img {
  margin-top: 30px;
  margin-bottom: 10%;
  width: 200rpx;
  height: 250rpx;
}


.bg-header {
  vertical-align: top;
  top: 0px;
  bottom: -10px;
  margin-top: 10%;
  height: 30px;
  width: 100%;
}


.icon {
  vertical-align: top;
  top: 0px;
  height: 30px;
  width: 30px;
  bottom: 0px;
}


.input {
  color: white;
}


.white_line {
  margin-top: 3%;
  margin-left: 12%;
  margin-right: 12%;
  height: 1px;
  background: white;
}



.bg{
  margin-top: 18%;
  position: absolute;
  width: 100%;
  height: 10px;
  background: #6699ff;
  margin-bottom: 5px;
}




.agree-p {
  color: #FFFFFF;
  text-decoration: underline;
}

p{
  font-size: 12px;
}


.border{
  box-shadow: 1px  1px 1px 1px #DDDDDD;
}

.item {
  margin-top: 20px;
  
}

.selected {
  margin-top: 50rpx;
  font-size: 30rpx;
}





